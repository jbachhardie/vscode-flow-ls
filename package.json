{
  "name": "flow-ls",
  "displayName": "flow-ls",
  "description": "Javascript Flow typechecking using Flow Language Server",
  "version": "0.0.1",
  "publisher": "jbachhardie",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Languages",
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact"
  ],
  "main": "./build/extension",
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "babel ./lib --out-dir=./build --source-maps",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "flow && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.6.1",
    "eslint-plugin-flowtype": "^2.40.1",
    "flow-bin": "^0.66.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "vscode-languageclient": "^3.5.0"
  },
  "dependencies": {
    "flow-language-server": "^0.4.0"
  }
}
